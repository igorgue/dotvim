{
  "suggest.triggerCompletionWait": 30,
  "suggest.autoTrigger": "always",
  "suggest.enablePreview": true,
  "suggest.echodocSupport": true,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.removeDuplicateItems": true,
  "diagnostic.virtualText": true,
  "diagnostic.messageTarget": "echo",
  "python.jediEnabled": false,
  "python.autoComplete.extraPaths": ["/home/igor/.cache/fakegir"],
  "python.analysis.disabled": ["inherit-non-class"],
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--max-line-length", "200"],
  "html.autoClosingTags": true,
  "git.enableGutters": true,
  "xml.java.home": "/usr/lib/jvm/default-java",
  "coc.preferences.colorSupport": true,
  "coc.preferences.messageLevel": "more",
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
  "omnisharp.useDotnet": true,
  "json.schemas": [
    {
      "fileMatch": [".vimspector.json"],
      "url": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json"
    }
  ],
  "languageserver": {
    "nim": {
      "command": "nimlsp",
      "filetypes": ["nim"]
    }
  }
}
