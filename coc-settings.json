{
  "coc.preferences.highlightTimeout": 200,
  "coc.preferences.messageLevel": "more",
  "diagnostic.level": "hint",
  "diagnostic.messageTarget": "echo",
  "diagnostic.virtualText": true,
  "elixir.pathToElixirLS": "~/.elixir-ls/release/language_server.sh",
  "git.enableGutters": true,
  "html.autoClosingTags": true,
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
  "python.analysis.disabled": ["inherit-non-class"],
  "python.autoComplete.extraPaths": ["~/.cache/fakegir"],
  "python.formatting.provider": "black",
  "python.jediEnabled": false,
  "python.linting.flake8Args": ["--max-line-length", "200"],
  "python.linting.flake8Enabled": true,
  "python.venvFolders": [".env", "env", ".venv", "venv"],
  "sql.database": "postgresql",
  "suggest.autoTrigger": "always",
  "suggest.echodocSupport": true,
  "suggest.enablePreselect": false,
  "suggest.enablePreview": true,
  "suggest.localityBonus": true,
  "suggest.removeDuplicateItems": true,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.triggerCompletionWait": 30,
  "xml.java.home": "/usr/lib/jvm/default-java",
  "json.schemas": [
    {
      "fileMatch": [".vimspector.json"],
      "url": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json"
    }
  ],
  "languageserver": {
    "nim": {
      "command": "nimlsp",
      "filetypes": ["nim"]
    }
  }
}
